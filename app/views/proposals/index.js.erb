$("<%=j render(partial: 'proposals/proposals', object: @proposals) %>").appendTo($(".infinite-page"));

// Update pagination link
<% if @proposals.last_page? %>
  $('.pagination').html("That's all, folks!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@proposals, 'Next Page', :remote => true) %>");
<% end %>
